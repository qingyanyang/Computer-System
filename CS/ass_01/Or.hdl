 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //a+b=Nand(Nand(a,a),Nand(b,b))
    Nand(a=a,b=a,out=c);
    Nand(a=b,b=b,out=d);
    Nand(a=c,b=d,out=out);
}
